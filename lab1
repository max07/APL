#include <iostream>
#include <cstdlib>
#include <stdio.h>

using namespace std;

int maxx=0;

class ar
{
public:

    int arr[100010];
    void populate();
    int compare();
    int del(int pos);
    void display();
};


/* *** Take values from user *** */
void ar::populate()
{

    for (int i=0;i<maxx;i++)
    {

        cin>>arr[i];
	}
}

int ar::compare()
{

//// *** Purging ***
    for (int i=0;i<maxx;++i)
    {
        for (int j=i+1;j<maxx;++j)   // compare element with next element(s)
        {
           if(arr[i]==arr[j])   //if same then delete
           {
                del(j);
                j--;
           }
        }
    }
////
return 0;
}




/* *** Delete the Duplicates *** */
int ar::del(int pos)
{
    for (int i=pos;i<maxx;i++)
    {
        arr[i]=arr[i + 1];   // over write the  value on delete element
                             // by shifting the index to index-1

    }
    maxx=maxx-1;             // reducing size of array subsequently
return 0;
}

/* *** Print Cuurent Data *** */
void ar::display()
{
    for (int i=0;i<maxx;i++)
        cout<<" "<<arr[i];
}


int main()
{
    //int *a;
    printf("\nEnter Size: ");  //Take size of array
    scanf("\n%d",&maxx);
cout<<"\nEnter data to Insert:  \n";
    ar m;
    //a=m.arr;
    m.populate();              // Take input
    cout<<endl;
    cout<<"\nInitial List: ";
    m.display();                //Display initial data
cout<<endl;

    m.compare();   ////////************ PURGING


cout<<endl;

cout<<"\nPurged List : ";
    m.display();                    // Display Final Data

cout<<endl;
return 0;
}
